{
  "name": "front-camp",
  "version": "1.0.0",
  "scripts": {
    "build": "npm run build:dev",
    "build:dev": "webpack --config config/webpack.dev.config.js",
    "build:prod": "webpack --config config/webpack.prod.config.js",
    "server": "npm run server:dev",
    "server:dev": "webpack-dev-server --config config/webpack.dev.config.js",
    "server:prod": "webpack-dev-server --config config/webpack.prod.config.js",
    "watch": "npm run watch:dev",
    "watch:dev": "npm run server:dev -- --watch",
    "watch:prod": "npm run server:prod -- --watch",
    "clean": "npm run clean:dist",
    "clean:dist": "rimraf -- dist/**/*",
    "mongo:server": "mongod --dbpath ./db/mongodb",
    "mongo:client": "mongo --host localhost --port 27017",
    "backend": "nodemon ./bin/www"
  },
  "devDependencies": {
    "babel-cli": "^6.18.0",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.16.0",
    "babel-preset-latest": "^6.16.0",
    "bootstrap": "^4.0.0-alpha.4",
    "bootstrap-loader": "^1.3.2",
    "bootstrap-sass": "^3.3.7",
    "bundle-loader": "^0.5.4",
    "clean-webpack-plugin": "^0.1.14",
    "copy-webpack-plugin": "^3.0.1",
    "css-loader": "^0.26.1",
    "exports-loader": "^0.6.3",
    "extract-loader": "^0.1.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "force-case-sensitivity-webpack-plugin": "^0.2.1",
    "html-loader": "^0.4.4",
    "html-webpack-plugin": "^2.24.1",
    "imports-loader": "^0.7.0",
    "jquery": "^3.1.1",
    "node-sass": "^4.0.0",
    "nodemon": "^1.11.0",
    "postcss-loader": "^1.2.1",
    "proxy-polyfill": "^0.1.6",
    "resolve-url-loader": "^1.6.1",
    "rimraf": "^2.5.4",
    "sass": "^0.5.0",
    "sass-loader": "^4.0.2",
    "style-loader": "^0.13.1",
    "text-loader": "^0.0.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-server": "^1.16.2",
    "webpack-merge": "^1.0.2",
    "express": "^4.14.0",
    "express-jwt": "^3.4.0",
    "express-sessions": "^1.0.5",
    "jsonwebtoken": "^6.2.0",
    "mongoose": "^4.7.2",
    "morgan": "^1.7.0",
    "angular-route": "^1.5.5",
    "angular-ui-bootstrap": "^1.3.2",
    "angularjs": "0.0.1",
    "body-parser": "~1.13.2",
    "client-sessions": "^0.7.0",
    "connect-mongodb": "^1.1.5",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "serve-favicon": "~2.3.0"
  }
}
